{
    "workbench.editor.empty.hint": "hidden",
    "biome.suggestInstallingGlobally": false,
    "editor.fontFamily": "Inter, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 13,
    "editor.lineHeight": 22,
    "editor.scrollBeyondLastLine": false,
    "editor.fastScrollSensitivity": 3.122,
    "editor.defaultFormatter": "biomejs.biome",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.biome": "explicit",
        "source.organizeImports.biome": "explicit"
    },
    "files.autoSave": "onFocusChange",
    "workbench.colorCustomizations": {
        "editor.background": "#1e1f22",
        "editor.foreground": "#a9b7c6"
    },
    "editor.foldingImportsByDefault": true,
    "gitlens.currentLine.enabled": false,
    "biome.requireConfiguration": true,
    "editor.minimap.enabled": false,
    "emmet.excludeLanguages": [
        "typescriptreact", "javascriptreact"
    ],
    "workbench.colorTheme": "Dark theme by KNeczaj",
    "editor.matchBrackets": "never",
    "material-icon-theme.folders.color": "#bcbec4",
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.saturation": 0.8,
    "material-icon-theme.folders.theme": "classic",
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "vscode_custom_css.imports": [
        "file://${userHome}/Library/Application Support/Code/User/css/index.css"
    ],
    "roo-cline.allowedCommands": [
        "git log",
        "git diff",
        "git show"
    ],
    "roo-cline.deniedCommands": [
        "yarn",
        "yarn add react-reconciler",
        "yarn add",
        "git commit",
        "ls",
        "echo",
        "grep",
        "head",
        "find",
        "mv",
        "yarn test:once",
        "yarn test:coverage",
        "git checkout HEAD -- src/index.ts",
        "git checkout",
        "git",
        "git checkout HEAD",
        "yarn lint",
        "yarn test",
        "npx tsc --noEmit",
        "npx",
        "npx tsc",
        "npm",
        "npm test",
        "npm test -- src/components/Schema.spec.tsx",
        "yarn biome check .",
        "npx @biomejs/biome check src/components/Object.ts"
    ],
    "editor.accessibilitySupport": "off",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.tabSize": 2,
    "workbench.editorAssociations": {
        "git-rebase-todo": "default"
    }
}
